{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FileProcessor": {
    "InputDirectory": "./input",
    "OutputDirectory": "./output",
    "ProcessingIntervalSeconds": 30,
    "SupportedExtensions": [".txt", ".csv", ".json", ".xml"],
    "MaxFileSizeBytes": 10485760
  }
}