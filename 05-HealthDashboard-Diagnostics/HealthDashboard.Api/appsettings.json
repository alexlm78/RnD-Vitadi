{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Diagnostics.HealthChecks": "Information"
    }
  },
  "AllowedHosts": "*",
  "HealthChecks": {
    "DatabaseTimeout": "00:00:30",
    "EnableDetailedErrors": true
  }
}
